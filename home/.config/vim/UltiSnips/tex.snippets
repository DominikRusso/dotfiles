%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% slightly raised and larger hyphen
\newcommand{\capitalhyphen}{\raisebox{0.22ex}{\resizebox{0.4em}{\height}{-}}\kern-0.07em}

% shorthand for siunitx's S column type
\newcolumntype{d}[1]{S[table-format={#1}]}

% dotted line using arydshln
\newcommand{\hdotline}{\hdashline[0.4pt/3pt]}

% redefine normal and var variants of greek letters
\let\origphi\phi
\let\phi\varphi
\let\origepsilon\epsilon
\let\epsilon\varepsilon

% definition for pifont glyphs
\def\crossmark{\text{\ding{55}}}

