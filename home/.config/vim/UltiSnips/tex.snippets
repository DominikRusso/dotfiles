\RequirePackage[l2tabu, orthodox]{nag} % warnings about obsolete latex commands

\documentclass[
	11pt,
	a4paper,
	USenglish
]{article}

%%%%%%%%%%%%%
% Essential %
%%%%%%%%%%%%%
\usepackage[T1]{fontenc} % 8-bit encoded font, native umlaut support
\usepackage[margin=25mm]{geometry} % interface to document dimensions
\usepackage[utf8]{inputenc} % allow UTF-8 characters as input
\usepackage{microtype} % subliminal refinements towards typographical perfection

%%%%%%%%%%%
% General %
%%%%%%%%%%%
\usepackage{array} % improvements to the column specification of array and tabular
\usepackage{arydshln} % dashed lines in array and tabular
\usepackage[style=alphabetic]{biblatex} % sophisticated bibliographies
\usepackage{booktabs} % publication quality tables
\usepackage{cancel} % diagonal strikethrough
\usepackage{caption} % customize captions in floating environments
\usepackage{changepage} % local margin adjustment
\usepackage{enumitem} % control layout of itemize, enumerate, description
\usepackage{fancyhdr} % extensive control of page headers and footers
\usepackage{float} % improved interface for floating objects
\usepackage{hyperref} % embedded hypertext links
\usepackage[iso]{isodate} % non-insane date format
% \usepackage{parskip} % zero parindent and non-zero parskip
\usepackage{siunitx} % units and decimal point aware column specification
\usepackage{tabularx} % more versatile version of tabular*
\usepackage{titlesec} % customize section titles
\usepackage[dvipsnames]{xcolor} % driver independent color extension

% options for hyperref package
\hypersetup{
	colorlinks = true, % color links instead of enclosing in boxes
	citecolor  = {green!50!black}, % color of citations
	linkcolor  = {green!50!black}, % color of internal links
	urlcolor   = black, % color for external hyperlinks
}

% options for isodate package
\isodash{\capitalhyphen} 

%%%%%%%%
% Code %
%%%%%%%%
\usepackage{algpseudocode} % pseudocode
\usepackage{cryptocode} % pseudocode and protocols in cryptography
