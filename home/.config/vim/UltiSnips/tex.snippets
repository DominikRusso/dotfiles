\usepackage{mathdots} % font size respecting dots in math mode
\usepackage{mathrsfs} % support for ralph smith's formal script in math mode
\usepackage{pifont} % postscript standard symbols and dingbats
\usepackage[tableaux]{prooftrees} % proof trees in the style of the semantic tableau
\usepackage{stmaryrd} % additional symbols for math and computer science

