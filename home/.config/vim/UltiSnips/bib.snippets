snippet article "Article" b
@article{${1:key},
	author={${2:authors, separated by "and"}},
	title={${3:title}},
	journal={${4:journal}},
	year={${5:YYYY}},

	month={${6:month}},
	number={${7:NN}},
	pages={${8:NN}--${9:NN}},
	volume={${10:NN}}
}
$0
endsnippet

##########

snippet book "Book" b
@book{${1:key},
	author={${2:authors, separated by "and"}},
	title={${3:title}},
	publisher={${4:publisher}},
	year={${5:YYYY}},

	address={${6:location}},
	edition={${7:edition}},
	number={${9:number}},
	//volume={${13:volume}},
	pages={${10:NN}--${11:NN}},
	series={${12:series}},
}
$0
endsnippet

