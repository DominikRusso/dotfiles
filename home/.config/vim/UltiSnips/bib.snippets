snippet incollection "InCollection" b
@incollection{${1:key},
	author={${2:authors of article, separated by "and"}},
	title={${3:title of article}},
	booktitle={${4:title of book containing article}},
	publisher={${5:publisher}},
	year={${6:YYYY}},

	address={${7:location}},
	chapter={${8:chapter}},
	edition={${9:edition}},
	editor={${10:editor}},
	number={${11:number}},
	//volume={${12:volume}},
	pages={${13:NN}--${14:NN}},
	series={${15:series}},
}
$0
endsnippet

